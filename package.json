{
  "name": "@react-playground/source",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://cisstech.github.io/react-playground",
  "scripts": {
    "nx": "nx",
    "start": "nx run app:serve",
    "build": "nx run app:build --prod",
    "deploy" : "gh-pages -d dist/apps/app",
    "test": "nx run-many --target=test --all --coverage=true --coverageReporters=lcov --watch=false --browsers=ChromeHeadless && node coverage-merger.js",
    "lint": "nx report && nx run-many --target=lint --all",
    "lint:fix": "nx report && nx run-many --target=lint --all --fix",
    "format": "nx format:write",
    "format:write": "prettier . --write",
    "format:check": "prettier . --check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "clear": "yarn clear:nx && yarn clear:modules && yarn clear:webpack && yarn clear:dist",
    "clear:nx": "rimraf -rf ./node_modules/.cache/nx",
    "clear:modules": "rimraf -rf ./node_modules",
    "clear:webpack": "rimraf -rf ./node_modules/.cache/webpack",
    "clear:dist": "rimraf -rf ./dist/"
  },
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@swc/helpers": "~0.5.2",
    "antd": "^5.11.3",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.20.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@nx/eslint": "17.1.3",
    "@nx/eslint-plugin": "17.1.3",
    "@nx/js": "17.1.3",
    "@nx/react": "17.1.3",
    "@nx/vite": "17.1.3",
    "@nx/workspace": "17.1.3",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-v8": "~0.34.6",
    "@vitest/ui": "~0.34.6",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "^6.1.0",
    "jsdom": "~22.1.0",
    "nx": "17.1.3",
    "prettier": "^2.6.2",
    "sass": "^1.55.0",
    "typescript": "~5.2.2",
    "vite": "~4.3.9",
    "vitest": "~0.34.6"
  }
}
